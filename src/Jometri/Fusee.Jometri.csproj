<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>Fusee.Jometri</RootNamespace>
    <PackageId>Fusee.Jometri</PackageId>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Description>Fusee Jometri</Description>
  </PropertyGroup>

  
  <PropertyGroup>
    <OutputPath>$(BaseOutputPath)\Libraries</OutputPath>
    <DocumentationFile>$(OutputPath)\$(RootNamespace).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="$(FuseeEngineRoot)\src\Base\Common\Fusee.Base.Common.csproj">
